.second { height: 40vh; width: 100%; display: flex; }
#second_1 {
height: 40vh;
width: 50%;
text-align: center; border: 1px solid black; }
.second_2 {
height: 40vh;
width: 50%;
text-align: center; border: 1px solid black; }
